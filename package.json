{
  "name": "vue3-animated-carousel",
  "version": "1.0.6",
  "description": "Carousel made with Vue 3 Composition API and Typescript.",
  "main": "dist/vue3-animated-carousel.ssr.js",
  "browser": "dist/vue3-animated-carousel.esm.js",
  "module": "dist/vue3-animated-carousel.esm.js",
  "unpkg": "dist/vue3-animated-carousel.min.js",
  "types": "types.d.ts",
  "files": [
    "dist/*",
    "types.d.ts",
    "src/**/*.vue",
    "src/**/*.ts",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "keywords": ["carousel", "vue3", "composition api", "mouse drag", "snap", "animation", "flexbox", "vertical", "typescript"],
  "license": "(MIT)",
  "author": {
      "name": "Gustavo da Cruz Vieira",
      "email" : "gustavodacrvi@rubble.com"
    },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/Gustavodacrvi/vue3-animated-carousel.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "types": "rimraf dist typings && tsc --emitDeclarationOnly && rollup -c",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/cli-plugin-babel": "^4.5.7",
    "@vue/cli-plugin-typescript": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/compiler-sfc": "^3.0.0",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.30.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "typescript": "^3.8.3",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=10"
  }
}
